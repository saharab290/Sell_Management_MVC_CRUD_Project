@model List<EIP_A.Models.DeletedSell>
@{
}
<div>
    <br />
    <h2 class="fw-bold mb-2 text-uppercase text-center">Deleted Sell List</h2>
</div>
<br />
<div class="container">
    <table class="table table-striped">
        <thead class="bg-dark text-white text-center">
            <tr>
                <th>Id</th>
                <th>Product Name</th>
                <th>Quantity</th>
                <th>Created At</th>
                <th>Created By</th>
                <th>Modeified At</th>
                <th>Modified By</th>
                <th>Total Price</th>
            </tr>
        </thead>
        <tbody class="text-center">

            @foreach (var sell in Model)
            {

                <tr>
                    <td>S-@sell.Id</td>
                    <td>@sell.ProductName</td>
                    <td>@sell.Quantity</td>
                    <td>@sell.CreatedAt</td>
                    <td>@sell.CreatedBy</td>
                    <td>@sell.ModifiedAt</td>
                    <td>@sell.ModifiedBy</td>
                    <td></td>
                </tr>

            }
        </tbody>
    </table>
</div>
